<Window x:Class="acme.wpfapp.FrameNavigationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:acme.wpfapp"
        mc:Ignorable="d"
        Title="FrameNavigationWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Click="ExternalUriToPageNoVM_OnClick">External Uri to Page No VM</Button>
            <Button Click="ExternalUriToPageSimpleVM_OnClick">External Uri to Page Simple VM</Button>

        </StackPanel>
        <Frame x:Name="NavigationFrame">
            
        </Frame>
    </DockPanel>
</Window>
