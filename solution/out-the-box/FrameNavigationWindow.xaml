<Window x:Class="out_the_box.FrameNavigationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:out_the_box"
        mc:Ignorable="d"
        Title="FrameNavigationWindow" Height="450" Width="800">
    <DockPanel>
        <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
            <Button Click="UriToPageNoVM_OnClick">Uri to Page No VM</Button>
            <Button Click="UriToPageSimpleVM_OnClick">Uri to Page Simple VM</Button>
            
            <Button Click="ContentPageNoVM_OnClick">New Page No VM</Button>
            <Button Click="ContentPageSimpleVM_OnClick">New Page Simple VM</Button>
        </StackPanel>
        <Frame x:Name="NavigationFrame">
            
        </Frame>
    </DockPanel>
</Window>
